{
  "name": "supernova-angular-1.5.x-es6-starter",
  "version": "0.0.2",
  "description": "starter kit for angular > 1.5.x, > es6, karma and jasmine",
  "main": "src/index.html",
  "directories": {},
  "scripts": {
    "e2e": "protractor protractor.conf.js",
    "e2ed": "protractor debug protractor.conf.js",
    "e2e:remote": "protractor ./gulp/config/protractor.conf.bs.js",
    "clean": "npm run clean:dist && rimraf node_modules/",
    "clean:dist": "rimraf dist/",
    "test": "karma start",
    "testd": "DEBUG=true npm test",
    "bdd": "BDD=true npm test",
    "start": "npm run dev & npm run dev:server",
    "wpw": "webpack -d --watch",
    "dogen": "gulp dogen",
    "dev": "webpack-dev-server --hot --inline --progress --colors --content-base src --port 9001",
    "prod": "npm run clean:dist && webpack --config=webpack.config.production.js -p",
    "prod:serve": "lite-server -c config/prod-config.json",
    "dev:server": "json-server --watch tests/mocks/db.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orizens/supernova-angular-1.5.x-es6-starter.git"
  },
  "keywords": [
    "angular",
    "karma",
    "es6",
    "jasmine"
  ],
  "author": "Oren Farhi",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-local-storage": "^0.2.2",
    "angular-mocks": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angular-ui-bootstrap": "^1.1.2",
    "angular-ui-router": "^1.0.0-beta.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "bootstrap": "^3.3.6",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "gulp": "3.9.1",
    "gulp-dogen": "0.1.9",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.17.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.2.3",
    "json-server": "^0.8.21",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.1",
    "karma-clear-screen-reporter": "^1.0.0",
    "karma-jasmine": "^0.3.6",
    "karma-json-fixtures-preprocessor": "0.0.5",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.13",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lite-server": "^2.2.2",
    "ng-annotate-loader": "^0.1.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "ngtemplate-loader": "^1.3.1",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.1.0",
    "selenium-webdriver": "^2.47.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
